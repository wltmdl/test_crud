<% for(var i = 0; i < Users.length; i++) {%>
<tr>
    <td><%= i + 1%></td>
    <td><%= Users[i].name %></td>
    <td><%= Users[i].email %></td>
    <td><%= Users[i].gender %></td>
    <td><%= Users[i].status %></td>
    <td>
        <a href="/update-user?id=<%= Users[i]._id %>" class="btn">
            <span>Update</span>
        </a>
        <a class="btn delete" data-id=<%= Users[i]._id%> >
            <span>Delete</span>
        </a>
    </td>
</tr>
<% } %>